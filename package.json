{
  "name": "@pachyderm/js-pachyderm",
  "version": "0.0.1",
  "description": "Javascript client for Pachyderm",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "dev": "LOG_LEVEL=debug NODE_ENV=development nodemon --watch ./src --ext ts -- --files src/index.ts",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "npm run build && jest",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pachyderm/js-pachyderm"
  },
  "author": "Pachyderm <email@pachyderm> (https://github.com/pachyderm)",
  "license": "Pachyderm Community License",
  "bugs": {
    "url": "https://github.com/pachyderm/js-pachyderm/issues"
  },
  "homepage": "https://github.com/pachyderm/js-pachyderm",
  "keywords": [
    "pachyderm",
    "js-pachyderm",
    "es6"
  ],
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.7",
    "@types/jest": "^26.0.24",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.0",
    "certifi": "^14.5.15",
    "css-loader": "^5.2.6",
    "eslint": "^7.30.0",
    "file-loader": "^6.2.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "prettier-webpack-plugin": "^1.2.0",
    "style-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.1.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.43.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "4.0.0-beta.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/testMock.js",
      "\\.(css|less)$": "<rootDir>/scripts/testMock.js"
    }
  },
  "_moduleAliases": {
    "@js-pachyderm": "./dist"
  }
}
